#include<stdio.h>
int main(){
    int i,j,k,l;
    for(i=5;i>=1;i--){
        for(j=5;j>i;j--){
            printf(" ");
        }
        for(k=1;k<=5;k++){
            printf("*");
        }
        for(l=1;l<=2*(i-1);l++){
            printf(" ");
        }
        for(k=1;k<=5;k++){
            printf("*");
        }
        printf("\n");
    }
    for(i=1;i<=4;i++){
        for(j=4;j>i;j--){
            printf(" ");
        }
        for(k=1;k<=5;k++){
            printf("*");
        }
        for(l=1;l<=2*i;l++){
            printf(" ");
        }
        for(k=1;k<=5;k++){
            printf("*");
        }
        printf("\n");
    }
}
